<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LazyColumn">
        <!--仅用于预览时，展示的Item布局-->
        <attr name="layout" format="reference" />
        <!--仅用于预览时，展示的Item数量-->
        <attr name="itemCount" format="integer" />
        <attr name="android:background" />
        <!--是否隔离视图，false表示多个相同type_flag的LazyColumn adapter item布局可以复用-->
        <attr name="isolateViewTypes" format="boolean" />
        <!--用来标识复用相同adapter item布局的多个LazyColumn-->
        <attr name="type_flag" format="string" />
    </declare-styleable>

</resources>