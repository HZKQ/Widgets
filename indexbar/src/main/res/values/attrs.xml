<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IndexBar">
        <attr name="ib_text_size" format="dimension|reference" />
        <attr name="ib_min_text_size" format="dimension|reference" />
        <attr name="ib_text_color" format="color|reference" />
        <attr name="ib_letters" format="string|reference" />
        <attr name="ib_letter_spacing" format="float" />
        <attr name="ib_letter_style">
            <enum name="normal" value="0" />
            <enum name="bold" value="1" />
            <enum name="italic" value="2" />
            <enum name="bold_italic" value="3" />
        </attr>
    </declare-styleable>
</resources>